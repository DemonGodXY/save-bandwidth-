{
  "version": 2,
  "builds": [
    {
      "src": "api/index1.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/index1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index1.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["bom1"]
}
